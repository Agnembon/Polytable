{
  "name": "polytable",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "packageManager": "bun@1.3.1",
  "workspaces": ["./packages/*"],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "clean": "bunx rimraf --glob '**/node_modules' '**/.turbo' '**/dist'",
    "check:dependencies": "bunx npm-check-updates && turbo run check:dependencies",
    "update:dependencies": "bunx npm-check-updates -u && turbo run update:dependencies"
  },
  "devDependencies": {
    "turbo": "^2.6.0"
  }
}
